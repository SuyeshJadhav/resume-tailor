# GitHub API
RESUME_GITHUB__TOKEN=ghp_your_github_personal_access_token
RESUME_GITHUB__USERNAME=your_github_username
RESUME_GITHUB__MIN_COMMITS=5

# LLM Provider (openai | ollama | groq)
RESUME_LLM__PROVIDER=openai
RESUME_LLM__MODEL=gpt-4o-mini
RESUME_LLM__API_KEY=sk-your_openai_api_key_here
# RESUME_LLM__BASE_URL=http://localhost:11434  # For Ollama

# Build Configuration
RESUME_BUILD__TEMPLATE_PATH=templates/resume.tex.j2
RESUME_BUILD__OUTPUT_DIR=output

# Pipeline Settings
RESUME_MAX_PROJECTS=4
RESUME_DEBUG=false
